<h1>Edit Profile</h1>
<div>
  <%= form_with model: @user do |form| %>
    <div class="field">
      <%= form.label :first_name %>
      <%= form.text_field :first_name %>
      <%= form.label :last_name %>
      <%= form.text_field :last_name %>
    </div>

    <div class="field">
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>
    <%= form.submit "Save Changes" %>
  <% end %>
</div>
